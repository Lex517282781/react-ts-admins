.print-content
  margin: 0 auto
  .print-content-inner
    position: relative
  .content-head
    height: auto
  .content-body
    table
      thead
        background: rgba(242, 242, 242, 1)
      th
        text-align: center
        border: 1px solid #1a1a1a
        font-weight: 400
      // th:nth-child(1)
      //   width: 20%
      // th:nth-child(2)
      //   width: 10%
      // th:nth-child(3)
      //   width: 40%
      // th:nth-child(4)
      //   width: 10%
      // th:nth-child(5)
      //   width: 20%
      td
        font-size: 14px
        border: 1px solid #1a1a1a
        word-wrap: break-word
        word-break: break-all
        padding: 0
        span
          width: 14px
          display: inline-block
  .content-foot
    height: auto